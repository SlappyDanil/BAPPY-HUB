-- Scripts:

local function RMCVPDT_fake_script() -- GUI.Drag 
	local script = Instance.new('LocalScript', GUI)

	local button = script.Parent
	local userInputService = game:GetService("UserInputService")
	local dragging = false
	local dragInput, dragStart, startPos
	
	button.Active = true  -- Required to detect input
	button.Selectable = true
	
	button.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = button.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	button.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	
	userInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - dragStart
			button.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
				startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end)
	
end
coroutine.wrap(RMCVPDT_fake_script)()
local function ROCQE_fake_script() -- Toggle.RGB 
	local script = Instance.new('LocalScript', Toggle)

	local toggleButton = script.Parent  -- Refers to the Toggle button
	local runService = game:GetService("RunService")
	local hue = 0
	
	runService.RenderStepped:Connect(function(deltaTime)
		hue = (hue + deltaTime * 0.2) % 1  -- Adjust speed here
		toggleButton.BackgroundColor3 = Color3.fromHSV(hue, 1, 1)
	end)
	
end
coroutine.wrap(ROCQE_fake_script)()
local function KYTL_fake_script() -- Toggle.ToggleClick 
	local script = Instance.new('LocalScript', Toggle)

	local toggleButton = script.Parent
	local frame = toggleButton.Parent -- Frame containing the toggle button
	local targetFrame = frame.Parent:WaitForChild("GUI") -- The GUI frame to show/hide
	local TweenService = game:GetService("TweenService")
	
	local isOpen = true
	local tweenTime = 0.3  -- seconds
	
	toggleButton.MouseButton1Click:Connect(function()
		isOpen = not isOpen
	
		if isOpen then
			-- Show the target frame and animate it in from the right
			targetFrame.Visible = true
			local tween = TweenService:Create(targetFrame, TweenInfo.new(tweenTime), {
				Position = targetFrame:GetAttribute("OriginalPosition") or targetFrame.Position
			})
			tween:Play()
		else
			-- Store the original position the first time the toggle is clicked
			if not targetFrame:GetAttribute("OriginalPosition") then
				targetFrame:SetAttribute("OriginalPosition", targetFrame.Position)
			end
			-- Animate the target frame to slide off the screen to the right
			local tween = TweenService:Create(targetFrame, TweenInfo.new(tweenTime), {
				Position = targetFrame.Position + UDim2.new(1, 0, 0, 0)  -- Moves it off the screen to the right
			})
			tween:Play()
			tween.Completed:Connect(function()
				targetFrame.Visible = false
			end)
		end
	end)
	
end
coroutine.wrap(KYTL_fake_script)()
local function JCUITK_fake_script() -- Toggle.Drag 
	local script = Instance.new('LocalScript', Toggle)

	local button = script.Parent
	local userInputService = game:GetService("UserInputService")
	local dragging = false
	local dragInput, dragStart, startPos
	
	button.Active = true  -- Required to detect input
	button.Selectable = true
	
	button.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = button.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	button.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	
	userInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - dragStart
			button.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
				startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end)
	
end
coroutine.wrap(JCUITK_fake_script)()
